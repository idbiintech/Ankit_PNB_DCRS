/*
 * Skin: Light Green
 * ------------
 */
.skin-lightgreen .main-header .navbar {
  background-color: #39cccc;
}
.skin-lightgreen .main-header .navbar .nav > li > a {
  color: #ffffff;
}
.skin-lightgreen .main-header .navbar .nav > li > a:hover,
.skin-lightgreen .main-header .navbar .nav > li > a:active,
.skin-lightgreen .main-header .navbar .nav > li > a:focus,
.skin-lightgreen .main-header .navbar .nav .open > a,
.skin-lightgreen .main-header .navbar .nav .open > a:hover,
.skin-lightgreen .main-header .navbar .nav .open > a:focus {
  background: rgba(0, 0, 0, 0.1);
  color: #f6f6f6;
}
.skin-lightgreen .main-header .navbar .navbar-custom-menu > .nav {
  margin-right: 10px;
}
.skin-lightgreen .main-header .navbar .sidebar-toggle {
  color: #ffffff;
}
.skin-lightgreen .main-header .navbar .sidebar-toggle:hover {
  color: #f6f6f6;
  background: rgba(0, 0, 0, 0.1);
}
.skin-lightgreen .main-header .navbar .sidebar-toggle {
  color: #fff;
}
.skin-lightgreen .main-header .navbar .sidebar-toggle:hover {
  background-color: #555299;
}
@media (max-width: 767px) {
  .skin-lightgreen .main-header .navbar .dropdown-menu li.divider {
    background-color: rgba(255, 255, 255, 0.1);
  }
  .skin-lightgreen .main-header .navbar .dropdown-menu li a {
    color: #fff;
  }
  .skin-lightgreen .main-header .navbar .dropdown-menu li a:hover {
    background: #555299;
  }
}
.skin-lightgreen .main-header .logo {
  background-color: #33adad;
  color: #ffffff;
  border-bottom: 0px solid transparent;
}
.skin-lightgreen .main-header .logo > a {
  color: #ffffff;
}
.skin-lightgreen .main-header .logo:hover {
  background: #545096;
}
.skin-lightgreen .main-header li.user-header {
  background-color: #39cccc;
}
.skin-lightgreen .content-header {
  background: transparent;
}
.skin-lightgreen .user-panel > .info,
.skin-lightgreen .user-panel > .info > a {
  color: #fff;
}
.skin-lightgreen .sidebar-menu > li.header {
  color: #4b646f;
  background: #1a2226;
}
.skin-lightgreen .sidebar-menu > li > a {
  border-left: 3px solid transparent;
  margin-right: 1px;
}
.skin-lightgreen .sidebar-menu > li > a:hover,
.skin-lightgreen .sidebar-menu > li.active > a {
  color: #ffffff;
  background: #1e282c;
  border-left-color: #39cccc;
}
.skin-lightgreen .sidebar-menu > li > .treeview-menu {
  margin: 0 1px;
  background: #2c3b41;
}
.skin-lightgreen .wrapper,
.skin-lightgreen .main-sidebar,
.skin-lightgreen .left-side {
  background: #222d32;
}
.skin-lightgreen .sidebar a {
  color: #b8c7ce;
}
.skin-lightgreen .sidebar a:hover {
  text-decoration: none;
}
.skin-lightgreen .treeview-menu > li > a {
  color: #8aa4af;
}
.skin-lightgreen .treeview-menu > li.active > a,
.skin-lightgreen .treeview-menu > li > a:hover {
  color: #ffffff;
}
.skin-lightgreen .sidebar-form {
  border-radius: 3px;
  border: 1px solid #374850;
  margin: 10px 10px;
}
.skin-lightgreen .sidebar-form input[type="text"],
.skin-lightgreen .sidebar-form .btn {
  box-shadow: none;
  background-color: #374850;
  border: 1px solid transparent;
  height: 35px;
  -webkit-transition: all 0.3s cubic-bezier(0.32, 1.25, 0.375, 1.15);
  -o-transition: all 0.3s cubic-bezier(0.32, 1.25, 0.375, 1.15);
  transition: all 0.3s cubic-bezier(0.32, 1.25, 0.375, 1.15);
}
.skin-lightgreen .sidebar-form input[type="text"] {
  color: #666;
  border-top-left-radius: 2px !important;
  border-top-right-radius: 0 !important;
  border-bottom-right-radius: 0 !important;
  border-bottom-left-radius: 2px !important;
}
.skin-lightgreen .sidebar-form input[type="text"]:focus,
.skin-lightgreen .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  background-color: #fff;
  color: #666;
}
.skin-lightgreen .sidebar-form input[type="text"]:focus + .input-group-btn .btn {
  border-left-color: #fff;
}
.skin-lightgreen .sidebar-form .btn {
  color: #999;
  border-top-left-radius: 0 !important;
  border-top-right-radius: 2px !important;
  border-bottom-right-radius: 2px !important;
  border-bottom-left-radius: 0 !important;
}
